{
  "schemaVersion": 1,
  "id": "pvpbot",
  "version": "${version}",
  "name": "PVPBot",
  "description": "Carpet PVP without any extra carpet",
  "authors": [
    "gnembon, HerobaneNair"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/pvpbot/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "hero.bane.pvpbot.client.PVPBotClient"
    ],
    "main": [
      "hero.bane.pvpbot.PVPBot"
    ]
  },
  "mixins": [
    "pvpbot.mixins.json",
    {
      "config": "pvpbot.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}"
  },
  "breaks": {
    "carpet": "*",
    "carpetpvp": "*"
  }
}
